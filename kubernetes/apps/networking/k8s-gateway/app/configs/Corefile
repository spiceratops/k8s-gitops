        lameduck 5s
    }
    ready
    log . {
        class error
    }
    prometheus 0.0.0.0:9153
    k8s_gateway ${PRIVATE_DOMAIN} {
        apex k8s-gateway.networking
        resources Ingress Service
        fallthrough ${PRIVATE_DOMAIN}
